ws.on('disconnected', function (data) {
       var data = JSON.parse(data);
        $('#window div p:eq(0)').before("<p style='color:"+data.color+"'>User disconnected: "+data.iduser+":"+data.ip+"</p>");
        $('#'+data.iduser).remove();
});
ws.on('tasktodo', function (data) {
        console.log("barruan");
        var data = JSON.parse(data);
        var scripturl=data.script;
        var img=data.img;
        _taskid=data.taskid;
        console.log(_taskid);
        script=document.createElement("script");
        script.src=scripturl;
        document.body.appendChild(script);
});


        function generateData() {
          var value = Math.floor(Math.random() * (100 - 1 + 1)) + 1;
          return {
            id: (new Date() / 1000) + "-" + value + "-" + (Math.floor(Math.random() * (1000 - 1 + 1)) + 1),
            value: value,
          };
        }
</script>
